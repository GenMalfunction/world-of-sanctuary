  float fGold = (fDeadLevel*20);
  float fXP = ((fDeadLevel/iKillerLevel)*75);
    int iGold = FloatToInt(fGold);
    int iXP = FloatToInt(fXP);
 if (GetIsPC(oKiller)==TRUE)
 {
